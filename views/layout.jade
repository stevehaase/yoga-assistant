html5
html
	head
		title= pageTitle
	body
		header
			h1= pageTitle
		section.login
			a(href="/") Home
			ul
				if session.isLoggedIn
					li
						a(href="/logout") Logout
				else
					li
						a(href="/login") Login
					li
						a(href="/signup") Create Account		

		section.content
			block content
	
